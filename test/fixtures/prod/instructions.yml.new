  updated_at: '2015-02-15 21:40:02.927944'
  instruction_group_id: '37'
instructions_010:
  step_number: '3'
  details: Bake for 20 minutes at 175C.
  created_at: '2015-02-15 21:40:02.929573'
  updated_at: '2015-02-15 21:40:02.929573'
  instruction_group_id: '37'
instructions_011:
