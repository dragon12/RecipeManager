  updated_at: '2015-02-15 21:16:22.711584'
  instruction_group_id: '2'
instructions_005:
  step_number: '6'
  details: Bake at 170C for 65 minutes. Let stand for at least 5 minutes.
  created_at: '2015-02-15 21:16:22.713076'
  updated_at: '2015-02-15 21:16:22.713076'
  instruction_group_id: '2'
