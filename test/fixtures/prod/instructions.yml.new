  instruction_group_id: '49'
instructions_062:
  step_number: '5'
  details: Add the flour, cook for 1 minute.
  created_at: '2015-02-16 21:24:17.683186'
  updated_at: '2015-02-16 21:24:17.683186'
  instruction_group_id: '49'
instructions_063:
  step_number: '6'
  details: Whisk in the stock and cream, simmer for 2 minutes until thickened.
  created_at: '2015-02-16 21:24:17.685132'
  updated_at: '2015-02-16 21:24:17.685132'
