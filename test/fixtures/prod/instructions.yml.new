instructions_058:
  step_number: '9'
  details: Serve!
  created_at: '2015-02-15 22:30:19.213265'
  updated_at: '2015-02-15 22:30:19.213265'
  instruction_group_id: '48'
instructions_059:
  step_number: '2'
  details: Add mushrooms to the pan with a little butter, cook until moisture has
    evaporated fully - about 10 minutes.
  created_at: '2015-02-16 21:24:17.677392'
  updated_at: '2015-02-16 21:24:17.677392'
