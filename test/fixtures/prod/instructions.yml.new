  created_at: '2015-02-15 21:40:02.933339'
  updated_at: '2015-02-15 21:40:02.933339'
  instruction_group_id: '38'
instructions_013:
  step_number: '1'
  details: Combine the ricotta, parmesan and eggs with 1/2 tsp salt and some pepper.
