  updated_at: '2015-02-15 21:56:48.600092'
  instruction_group_id: '45'
instructions_037:
  step_number: '2'
  details: Cook the corn in oil for 4 minutes (until charred in spots).
  created_at: '2015-02-15 21:56:48.601463'
  updated_at: '2015-02-15 21:56:48.601463'
  instruction_group_id: '45'
