  updated_at: '2015-02-15 21:56:48.60589'
  instruction_group_id: '45'
instructions_041:
  step_number: '6'
  details: Simmer for 30 minutes.
  created_at: '2015-02-15 21:56:48.607126'
  updated_at: '2015-02-15 21:56:48.607126'
  instruction_group_id: '45'
